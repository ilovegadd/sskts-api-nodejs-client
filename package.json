{
  "name": "@motionpicture/sskts-api-nodejs-client",
  "version": "1.1.0",
  "description": "SSKTS API client library for Node.js",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "files": [
    "doc",
    "example",
    "lib"
  ],
  "directories": {
    "doc": "./doc",
    "lib": "./lib",
    "example": "./example",
    "test": "./test"
  },
  "scripts": {
    "build": "tsc",
    "check": "npm run tslint && nsp check",
    "clean": "rimraf ./lib npm-debug.log* ./test/**/*.js ./doc ./coverage ./.nyc_output",
    "example": "http-server ./example",
    "doc": "jsdoc -c jsdoc.json",
    "mocha": "mocha src/**/*.spec.ts",
    "coverage": "nyc mocha src/**/*.spec.ts | coveralls",
    "snyk": "snyk wizard",
    "test": "npm run check && npm run coverage",
    "tslint": "tslint --type-check --project tsconfig.json -c tslint.json --exclude \"**/*.spec.ts\" src/**/*.ts",
    "preversion": "npm run clean && npm run build && npm test",
    "version": "git add -A",
    "postversion": "git push origin --tags",
    "prepublishOnly": "npm run clean && npm run build && npm test && npm run doc"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ilovegadd/sskts-api-nodejs-client.git"
  },
  "keywords": [
    "sskts"
  ],
  "author": {
    "name": "Motionpicture co.,ltd."
  },
  "contributors": [
    {
      "email": "yamazaki@motionpicture.jp",
      "name": "Tetsu Yamazaki"
    }
  ],
  "license": "UNLICENSED",
  "devDependencies": {
    "@motionpicture/coa-service": "^4.0.3",
    "@motionpicture/gmo-service": "^4.0.1",
    "@motionpicture/sskts-domain": "^23.0.0-alpha.6",
    "@types/debug": "0.0.30",
    "@types/form-data": "^2.2.0",
    "@types/http-status": "^0.2.30",
    "@types/isomorphic-fetch": "0.0.34",
    "@types/mocha": "^2.2.43",
    "@types/nock": "^8.2.1",
    "@types/open": "0.0.29",
    "@types/power-assert": "^1.4.29",
    "@types/request": "^2.0.3",
    "coveralls": "^2.13.1",
    "http-server": "^0.10.0",
    "ink-docstrap": "^1.3.0",
    "jsdoc": "^3.5.5",
    "mocha": "^3.5.3",
    "nock": "^9.0.20",
    "nsp": "^2.8.0",
    "nyc": "^11.2.1",
    "open": "0.0.5",
    "power-assert": "^1.4.4",
    "rimraf": "^2.6.2",
    "snyk": "^1.41.1",
    "ts-node": "^3.3.0",
    "tslint": "^5.7.0",
    "tslint-microsoft-contrib": "^5.0.1",
    "typescript": "^2.5.2"
  },
  "dependencies": {
    "@motionpicture/sskts-api-abstract-client": "^1.1.0",
    "debug": "^3.0.1",
    "http-status": "^1.0.1",
    "isomorphic-fetch": "^2.2.1"
  }
}
