{
  "name": "@motionpicture/sasaki-api-nodejs",
  "version": "1.0.0",
  "description": "sskts API Client Library for Node.js",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "files": [
    "lib"
  ],
  "directories": {
    "doc": "./doc",
    "lib": "./lib",
    "example": "./example",
    "test": "./test"
  },
  "scripts": {
    "build": "tsc",
    "build-tools": "tsc -p tsconfig.tools.json",
    "check": "npm run tslint && nsp check && npm run snyk",
    "clean": "rimraf ./lib npm-debug.log* ./doc",
    "example": "http-server ./example",
    "jsdoc": "node_modules/.bin/jsdoc -c jsdoc.json",
    "mocha": "mocha",
    "nyc": "nyc mocha",
    "snyk": "snyk auth && snyk test --ignore-policy",
    "test": "echo \"Error: no test specified\" && exit 1",
    "tslint": "tslint --type-check --project tsconfig.json -c tslint.json src/**/*.ts",
    "preversion": "npm run clean && npm run build && npm test && npm run jsdoc",
    "version": "git add -A",
    "postversion": "git push origin --tags"
  },
  "repository": {
    "type": "git",
    "url": "https://m-p.backlog.jp/git/SSKTS/api-nodejs-client.git"
  },
  "keywords": [
    "sasaki-api"
  ],
  "author": {
    "name": "Motionpicture co.,ltd."
  },
  "contributors": [
    {
      "email": "yamazaki@motionpicture.jp",
      "name": "Tetsu Yamazaki"
    }
  ],
  "license": "UNLICENSED",
  "devDependencies": {
    "@motionpicture/coa-service": "^4.0.1",
    "@motionpicture/gmo-service": "^4.0.0",
    "@types/debug": "0.0.30",
    "@types/form-data": "^2.2.0",
    "@types/http-status": "^0.2.30",
    "@types/isomorphic-fetch": "0.0.34",
    "@types/mocha": "^2.2.42",
    "@types/nock": "^8.2.1",
    "@types/open": "0.0.29",
    "@types/power-assert": "^1.4.29",
    "@types/request": "^2.0.3",
    "http-server": "^0.10.0",
    "ink-docstrap": "^1.3.0",
    "jsdoc": "^3.5.4",
    "mocha": "^3.5.0",
    "nock": "^9.0.14",
    "nsp": "^2.7.0",
    "nyc": "^11.1.0",
    "open": "0.0.5",
    "power-assert": "^1.4.4",
    "rimraf": "^2.6.1",
    "snyk": "^1.40.2",
    "tslint": "^5.7.0",
    "tslint-microsoft-contrib": "^5.0.1",
    "typescript": "^2.5.2"
  },
  "dependencies": {
    "@motionpicture/sskts-factory": "git+https://tetsu:M0P!Xmopix@m-p.backlog.jp/git/SSKTS/factory.git#feature/SSKTS-566",
    "debug": "^3.0.1",
    "http-status": "^1.0.1",
    "isomorphic-fetch": "^2.2.1"
  },
  "nyc": {
    "check-coverage": true,
    "include": [
      "lib/**/*.js"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ]
  }
}
