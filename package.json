{
  "name": "@motionpicture/sskts-api-nodejs-client",
  "version": "4.4.0",
  "description": "SSKTS API Client Library for Node.js",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "files": [
    "doc",
    "example",
    "lib"
  ],
  "directories": {
    "doc": "./doc",
    "lib": "./lib",
    "example": "./example",
    "test": "./test"
  },
  "scripts": {
    "build": "tsc",
    "check": "npm run tslint",
    "clean": "rimraf ./lib \"npm-debug.log*\" ./doc ./coverage ./.nyc_output",
    "example": "http-server ./example",
    "doc": "jsdoc -c jsdoc.json",
    "mocha": "mocha",
    "nyc": "nyc mocha \"src/**/*.spec.ts\"",
    "coverage": "npm run nyc | coveralls",
    "snyk": "snyk wizard",
    "test": "npm run check && npm run coverage",
    "tslint": "tslint --project tsconfig.json -c tslint.json --exclude \"**/*.spec.ts\" \"src/**/*.ts\"",
    "preversion": "npm run clean && npm run build && npm test",
    "version": "git add -A",
    "postversion": "git push origin --tags",
    "prepublishOnly": "npm run clean && npm run build && npm test && npm run doc"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/motionpicture/sskts-api-nodejs-client.git"
  },
  "keywords": [
    "sskts"
  ],
  "author": {
    "name": "Motionpicture co.,ltd."
  },
  "contributors": [
    {
      "email": "yamazaki@motionpicture.jp",
      "name": "Tetsu Yamazaki"
    }
  ],
  "license": "ISC",
  "devDependencies": {
    "@motionpicture/coa-service": "^5.0.0",
    "@motionpicture/gmo-service": "^4.2.1",
    "@types/lodash.difference": "^4.5.4",
    "@types/mocha": "^5.2.5",
    "@types/open": "0.0.29",
    "coveralls": "^3.0.2",
    "http-server": "^0.11.1",
    "ink-docstrap": "^1.3.2",
    "jsdoc": "^3.5.5",
    "lodash.difference": "^4.5.0",
    "mocha": "^5.2.0",
    "nyc": "^12.0.2",
    "open": "0.0.5",
    "rimraf": "^2.6.2",
    "snyk": "^1.89.1",
    "ts-node": "^7.0.1",
    "tslint": "^5.12.1",
    "tslint-microsoft-contrib": "^6.0.0",
    "typescript": "^3.2.4"
  },
  "dependencies": {
    "@cinerino/api-nodejs-client": "^1.1.0-alpha.39",
    "@motionpicture/sskts-api-abstract-client": "^4.5.0-alpha.1"
  }
}
