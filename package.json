{
  "name": "@motionpicture/sskts-api-nodejs-client",
  "version": "3.0.0",
  "description": "SSKTS API client library for Node.js",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "files": [
    "doc",
    "example",
    "lib"
  ],
  "directories": {
    "doc": "./doc",
    "lib": "./lib",
    "example": "./example",
    "test": "./test"
  },
  "scripts": {
    "build": "tsc",
    "check": "npm run tslint && nsp check",
    "clean": "rimraf ./lib \"npm-debug.log*\" ./doc ./coverage ./.nyc_output",
    "example": "http-server ./example",
    "doc": "jsdoc -c jsdoc.json",
    "mocha": "mocha",
    "nyc": "nyc mocha \"src/**/*.spec.ts\"",
    "coverage": "npm run nyc | coveralls",
    "snyk": "snyk wizard",
    "test": "npm run check && npm run coverage",
    "tslint": "tslint --project tsconfig.json -c tslint.json --exclude \"**/*.spec.ts\" \"src/**/*.ts\"",
    "preversion": "npm run clean && npm run build && npm test",
    "version": "git add -A",
    "postversion": "git push origin --tags",
    "prepublishOnly": "npm run clean && npm run build && npm test && npm run doc"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/motionpicture/sskts-api-nodejs-client.git"
  },
  "keywords": [
    "sskts"
  ],
  "author": {
    "name": "Motionpicture co.,ltd."
  },
  "contributors": [
    {
      "email": "yamazaki@motionpicture.jp",
      "name": "Tetsu Yamazaki"
    }
  ],
  "license": "ISC",
  "devDependencies": {
    "@motionpicture/coa-service": "^4.1.0-alpha.0",
    "@motionpicture/gmo-service": "^4.1.0",
    "@motionpicture/mvtk-service": "^2.0.2",
    "@motionpicture/sskts-domain": "^23.3.0",
    "@types/debug": "0.0.30",
    "@types/form-data": "^2.2.1",
    "@types/http-status": "^0.2.30",
    "@types/isomorphic-fetch": "0.0.34",
    "@types/lodash.difference": "^4.5.3",
    "@types/mocha": "^5.2.0",
    "@types/moment-timezone": "^0.5.3",
    "@types/nock": "^9.1.3",
    "@types/open": "0.0.29",
    "@types/power-assert": "^1.5.0",
    "@types/request": "^2.47.0",
    "coveralls": "^3.0.1",
    "http-server": "^0.10.0",
    "ink-docstrap": "^1.3.2",
    "jsdoc": "^3.5.5",
    "lodash.difference": "^4.5.0",
    "mocha": "^5.1.1",
    "moment-timezone": "^0.5.14",
    "nock": "^9.2.6",
    "nsp": "^3.2.1",
    "nyc": "^11.8.0",
    "open": "0.0.5",
    "power-assert": "^1.5.0",
    "rimraf": "^2.6.2",
    "snyk": "^1.80.1",
    "ts-node": "^6.0.3",
    "tslint": "^5.10.0",
    "tslint-microsoft-contrib": "^5.0.3",
    "typescript": "^2.8.3"
  },
  "dependencies": {
    "@motionpicture/sskts-api-abstract-client": "^3.1.0-alpha.4",
    "debug": "^3.1.0",
    "http-status": "^1.0.1",
    "isomorphic-fetch": "^2.2.1"
  }
}
